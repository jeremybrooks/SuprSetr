<shortcuts>

  <programGroup defaultName="SuprSetr" location="applications"/>

    
  <shortcut
     name="SuprSetr"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH/bin/launch_suprsetr.sh"
     commandLine=""
     workingDirectory="$INSTALL_PATH/bin"
     description="SuprSetr"
     iconFile="$INSTALL_PATH/images/S-32.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="false"
     KdeSubstUID="false"
     initialState="noShow">
   </shortcut>

